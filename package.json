{
  "name": "@awey/maxios",
  "description": "Use axios to fetch data, but configure every api with 4 levels of configurations.",
  "version": "2.1.5",
  "author": {
    "name": "Awey",
    "email": "bboyawey@sina.cn"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "server": "npx nodemon demo/server/index.js",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:BboyAwey/maxios.git"
  },
  "files": [
    "lib",
    "README.md",
    "README_zh.md"
  ],
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs.js"
    },
    "./react": {
      "types": "./lib/react.d.ts",
      "import": "./lib/react.js",
      "require": "./lib/react.cjs.js",
      "peerDependencies": {
        "react": ">=16.8.0"
      }
    },
    "./vue": {
      "types": "./lib/vue.d.ts",
      "import": "./lib/vue.js",
      "require": "./lib/vue.cjs.js",
      "peerDependencies": {
        "vue": ">=3.0.0"
      }
    }
  },
  "devDependencies": {
    "@koa/router": "^12.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "jest": "^29.0.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "nodemon": "^2.0.20",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.7.1",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@awey/dache": "^1.0.0",
    "axios": "^1.7.9",
    "fast-deep-equal": "^3.1.3"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
